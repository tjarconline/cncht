<script>
var timetable = new Timetable();
timetable.setScope(18, 4); // optional, only whole hours between 0 and 23
timetable.addLocations(['Bibelot', 'Bibelot Power Stage', 'Bibelot Brandstof', 'Popcentrale', 'Popcentrale Mainstage', 'Popcentrale Gang', 'ToBe', 'ToBe 1ste Etage', 'ToBe K2', 'Kunstmin K1', 'Kunstmin Machine 3', 'Kunstmin', 'Kunstmin Kleine Zaal', 'Dordts Museum', 'Hof van Holland', 'The Movies', 'The Movies Divas Foyer', 'The Movies Filmlounge', 'Bibliotheek', 'Bibliotheek Blauwe Kamer', 'Bibliotheek DordtLAB', 'Bibliotheek Jongerenvloer', 'Bibliotheek Cafe Americain', 'Onderwijsmuseum', 'DOOR', 'DJS', 'Grote Kerk', 'Statenplein', 'Statenplein Via Cultura', 'Statenplein ICONOCLASH']);

var renderer = new Timetable.Renderer(timetable);
renderer.draw('.timetable'); // any css selector
</script>
