<script>
var timetable = new Timetable();
timetable.setScope(18, 4); // optional, only whole hours between 0 and 23
timetable.addLocations(['Bibelot', 'Bibelot Power Stage', 'Brandstof', 'Popcentrale', 'Popcentrale Mainstage', 'Popcentrale Gang', 'ToBe', 'ToBe 1ste Etage', 'ToBe K2', 'Kunstmin K1', 'Kunstmin Machine 3', 'Kunstmin', 'Kunstmin Kleine Zaal', 'Dordts Museum', 'Hof van Holland', 'The Movies', 'The Movies Diva's Foyer', 'The Movies Filmlounge', 'Bibliotheek', 'Bibliotheek Blauwe Kamer', 'Bibliotheek DordtLAB', 'Bibliotheek Jongerenvloer', 'Bibliotheek Cafe Americain', 'Onderwijsmuseum', 'DOOR', 'DJS', 'Grote Kerk', 'Statenplein', 'Statenplein Via Cultura', 'Statenplein ICONOCLASH']); 

var options = { url: 'https://cultuurnacht.webflow.io/', class: 'normal', onClick: function(event, timetable, clickEvent) {} }; timetable.addEvent('20:00 DE NACHTTUIN', 'Bibliotheek', new Date(2015,7,17,20,00), new Date(2015,7,17,23,00), options);
var options = { url: 'https://cultuurnacht.webflow.io/', class: 'normal', onClick: function(event, timetable, clickEvent) {} }; timetable.addEvent('21:00 HIPHOP IN DE BIEB', 'Bibliotheek Blauwe Kamer', new Date(2015,7,17,21,00), new Date(2015,7,17,21,40), options);
var options = { url: 'https://cultuurnacht.webflow.io/', class: 'weird', onClick: function(event, timetable, clickEvent) {} }; timetable.addEvent('20:00 HET RUIMTELAB', 'Bibliotheek DordtLAB', new Date(2015,7,17,20,00), new Date(2015,7,17,20,40), options);
var options = { url: 'https://cultuurnacht.webflow.io/', class: 'weird', onClick: function(event, timetable, clickEvent) {} }; timetable.addEvent('20:00 SPOKEN WORD / KAMPVUURVERHALEN', 'Bibliotheek Jongerenvloer', new Date(2015,7,17,20,00), new Date(2015,7,17,23,00), options);
var options = { url: 'https://cultuurnacht.webflow.io/', class: 'normal', onClick: function(event, timetable, clickEvent) {} }; timetable.addEvent('20:00 BLAADPRAAT: MENSENBIEB CANDLELIGHT', 'Bibliotheek Cafe Americain', new Date(2015,7,17,20,00), new Date(2015,7,17,23,00), options);

var renderer = new Timetable.Renderer(timetable);
renderer.draw('.timetable'); // any css selector
</script>
